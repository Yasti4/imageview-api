{
    "name": "imageview-server",
    "version": "1.0.0",
    "description": "An API for an image sharing platform.",
    "scripts": {
        "start": "./node_modules/.bin/nodemon ./src/app.js",
        "migrate": "node ./src/scripts/migrations.js --migrate",
        "migrate:rollback": "node ./src/scripts/migrations.js --rollback",
        "make:migration": "node ./src/scripts/migrations.js --make",
        "db:seed": "node ./src/scripts/seeds.js --run",
        "make:seeder": "node ./src/scripts/seeds.js --make"
    },
    "author": "Adrià Muñoz, Denís Navarro",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/Yasti4/imageview-api.git"
    },
    "license": "MIT",
    "dependencies": {
        "apollo-server-express": "^1.3.2",
        "bcrypt": "^1.0.3",
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "graphql": "^0.13.2",
        "graphql-date": "^1.0.3",
        "graphql-fields": "^1.0.2",
        "graphql-tools": "^2.23.1",
        "jwt-simple": "^0.5.1",
        "moment": "^2.21.0",
        "mysql2": "^1.5.3",
        "sequelize": "^4.37.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^1.0.0",
        "@types/body-parser": "^1.16.8",
        "@types/dotenv": "^4.0.2",
        "@types/faker": "^4.1.2",
        "@types/graphql": "^0.12.4",
        "@types/jwt-simple": "^0.5.33",
        "@types/moment": "^2.13.0",
        "@types/mysql": "^2.15.3",
        "@types/node": "^9.4.7",
        "@types/sequelize": "^4.27.10",
        "faker": "^4.1.0",
        "nodemon": "^1.17.2",
        "sequelize-cli": "^4.0.0"
    }
}